mixin exerciseRow(exercise)
  tr
    td=exercise.name
    td=exercise.submit_date
    td
      if exercise.corrected
        i.far.fa-check-circle.corrected.student-exercise-element
      else
        i.far.fa-times-circle.not-submitted.student-exercise-element
    td=exercise.cash_reward
    td=exercise.point_reward